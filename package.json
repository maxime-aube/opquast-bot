  "dependencies": {
    "@discordjs/builders": "^0.9.0",
    "@discordjs/rest": "^0.2.0-canary.0",
    "cron-job-manager": "^2.2.1",
    "discord-api-types": "^0.25.2",
    "discord.js": "^13.3.1",
    "winston": "^3.3.4"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "nodemon": "^2.0.15"
  }
