{
  "name": "opquastbot",
  "version": "1.0.0",
  "description": "Un bot discord pédagogique non officiel pour la certification Opquast (qualité web)",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js --ignore 'publication-subscriptions.json' --ignore 'history/*.json'",
